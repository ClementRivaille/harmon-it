{
  "globals": {
    // Put things like jQuery, etc
    "jQuery": true,
    "$": true
  },
  "env": {
    // I write for browser
    "browser": true,
    // in CommonJS
    "node": true,
  },
  // To give you an idea how to override rule options:
  "rules": {
    "space-before-function-paren": 0
  },
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "standard", "standard-babel"],
  "plugins": [
    "babel",
    "standard"
  ]
}
